apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: boardk8s
spec:
  ingressClassName: nginx
  rules:
    - host: [host-name]
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: boardk8s
                port: 
                  number: 80
